<!DOCTYPE html>
<html>

<head>
    <title>{% block title %}Welcome!{% endblock %}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    {{ encore_entry_script_tags('section-frontend') }}

    <link rel="apple-touch-icon" href="{{ asset('build/img/apple-icon.png') }}">
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('build/img/favicon.ico') }}">

    {% block stylesheets %}
        {{ encore_entry_link_tags('section-frontend') }}
